{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "appointer/swaggervel": "dev-master",
        "coconutcraig/laravel-postmark": "^2.11",
        "crystoline/laravel-rest-api": "^2.0",
        "doctrine/dbal": "~2.3",
        "fakerphp/faker": "^1.9.1",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "infyomlabs/adminlte-templates": "^3.0",
        "infyomlabs/laravel-generator": "^3.0",
        "infyomlabs/swagger-generator": "dev-master",
        "kingflamez/laravelrave": "^4.2",
        "laravel/framework": "^8.65",
        "laravel/sanctum": "^2.11",
        "laravel/tinker": "^2.5",
        "laravelcollective/html": "^6.2",
        "pusher/pusher-php-server": "^7.0",
        "spatie/laravel-permission": "^5.3",
        "tymon/jwt-auth": "^1.0",
        "yajra/laravel-datatables-oracle": "^9.19"
    },
    "require-dev": {
        "bennett-treptow/laravel-migration-generator": "^4.3",
        "facade/ignition": "^2.5",
        "kitloong/laravel-migrations-generator": "^6.10",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "repositories": [
    {
        "type": "vcs",
        "url":  "git@github.com:InfyOmLabs/swaggervel.git"
    }
],
    "minimum-stability": "dev",
    "prefer-stable": true
}
